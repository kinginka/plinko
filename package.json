{
  "name": "plinko",
  "version": "1.0.0",
  "description": "A networked plinko game",
  "main": "src/server/server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel -presets es2015 -d lib/ src/",
    "build": "npx webpack && npm run compile",
