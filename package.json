  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "http": "0.0.0",
    "localtunnel": "^1.9.0",
    "socket.io": "^2.1.1",
